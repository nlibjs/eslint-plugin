{
    "name": "@nlib/eslint-plugin",
    "publishConfig": {
        "access": "public"
    },
    "version": "1.0.11",
    "license": "Apache-2.0",
    "author": {
        "name": "Kei Ito",
        "email": "kei.itof@gmail.com",
        "url": "https://github.com/kei-ito"
    },
    "homepage": "https://github.com/nlibjs/eslint-plugin",
    "repository": "https://github.com/nlibjs/eslint-plugin",
    "engines": {
        "node": ">=12"
    },
    "main": "./index.js",
    "exports": {
        ".": "./index.js",
        "./package.json": "./package.json"
    },
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "scripts": {
        "test": "ts-node test/no-globals.test.ts",
        "version": "run-s version:changelog version:add",
        "version:changelog": "nlib-changelog --output CHANGELOG.md",
        "version:add": "git add ."
    },
    "peerDependencies": {
        "eslint": ">= 3"
    },
    "devDependencies": {
        "@nlib/changelog": "0.1.9",
        "@nlib/eslint-plugin": "file:.",
        "@nlib/lint-commit": "0.1.3",
        "@types/eslint": "7.2.7",
        "@types/node": "14.14.31",
        "@typescript-eslint/parser": "4.16.1",
        "eslint": "7.21.0",
        "husky": "4.3.8",
        "npm-run-all": "4.1.5",
        "ts-node": "9.1.1",
        "typescript": "4.2.3"
    },
    "husky": {
        "hooks": {
            "commit-msg": "nlib-lint-commit --input HUSKY_GIT_PARAMS"
        }
    },
    "renovate": {
        "extends": [
            "github>nlibjs/renovate-config"
        ]
    }
}
